<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 layout="topleft"
 left="1"
 top="1"
 width="540"
 label="Move"
 name="move_panel">
<tab_container
 label="Move and View"
 layout="topleft"
 follows="all"
 top="5"
 bottom="-1"
 left="1"
 right="-1"
 name="tabs"
 tab_min_width="50"
 tab_position="top">

    <!--View-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="View"
     name="tab-view">
        <icon
         follows="left|top"
         height="18"
         image_name="Cam_FreeCam_Off"
         layout="topleft"
         name="camera_icon"
         mouse_opaque="false"
         visible="true"
         width="18"
         left="10"
         top="10"/>
        <text
         follows="top|left|right"
         height="16"
         layout="topleft"
         left="38"
         name="preset_camera_label"
         top_delta="0"
         width="75">
         Preset in use:
        </text>
        <text
         follows="top|left|right"
         height="16"
         layout="topleft"
         left_pad="10"
         name="preset_camera_text"
         width="120">
         (None)
        </text>
        <slider
         can_edit_text="true"
         control_name="CameraAngle"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.025"
         initial_value="1.57"
         layout="topleft"
         label_width="100"
         label="View angle"
         left="38"
         max_val="2.97"
         min_val="0.17"
         name="camera_fov"
         show_text="true"
         top_pad="5"
         width="275" />
        <slider
         can_edit_text="true"
         control_name="CameraOffsetScale"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.025"
         initial_value="1"
         layout="topleft"
         label="Distance"
         left_delta="0"
         label_width="100"
         max_val="3"
         min_val="0.5"
         name="camera_offset_scale"
         show_text="true"
         width="275"
         top_pad="5"/>
        <slider
         can_edit_text="true"
         control_name="ZoomTime"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.1"
         initial_value="0.4"
         layout="topleft"
         label="Transition Time"
         left_delta="0"
         label_width="100"
         max_val="4"
         min_val="0"
         name="camera_transition_time"
         show_text="true"
         width="275"
         top_pad="5"/>
        <slider
         can_edit_text="true"
         control_name="CameraPositionSmoothing"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.1"
         initial_value="1"
         layout="topleft"
         label="Smoothing"
         left_delta="0"
         label_width="100"
         max_val="9"
         min_val="0"
         name="camera_position_smoothing"
         show_text="true"
         width="275"
         top_pad="5"/>

        <text
         follows="top|left|right"
         height="16"
         layout="topleft"
         left="322"
         name="camera_offset_label"
         tool_tip="Initial camera offset from avatar in Rear View"
         top="10"
         width="100">
          Camera offset:
        </text>
        <spinner
         height="20"
         label="X"
         label_width="10"
         follows="top|left"
         left="322"
         name="camera_x"
         top_pad="5"
         min_val="-1e+007"
         max_val="1e+007"
         width="67"/>
        <spinner
         height="20"
         label="Y"
         label_width="10"
         follows="top|left"
         name="camera_y"
         left_pad="4"
         min_val="-1e+007"
         max_val="1e+007"
         width="67"/>
        <spinner
         height="20"
         label="Z"
         label_width="10"
         follows="top|left"
         name="camera_z"
         left_pad="4"
         min_val="-1e+007"
         max_val="1e+007"
         width="67"/>
        <text
         follows="top|left|right"
         height="16"
         layout="topleft"
         left="322"
         name="camera_focus_offset_label"
         tool_tip="Initial focus point offset relative to avatar for the camera preset Rear View (x-axis is forward)"
         top_pad="18"
         width="100">
         Focus offset:
        </text>
        <spinner
         height="20"
         label="X"
         label_width="10"
         follows="top|left"
         left="322"
         name="focus_x"
         top_pad="5"
         min_val="-1e+007"
         max_val="1e+007"
         width="67"/>
        <spinner
         height="20"
         label="Y"
         label_width="10"
         follows="top|left"
         name="focus_y"
         left_pad="4"
         min_val="-1e+007"
         max_val="1e+007"
         width="67"/>
        <spinner
         height="20"
         label="Z"
         label_width="10"
         follows="top|left"
         name="focus_z"
         left_pad="4"
         min_val="-1e+007"
         max_val="1e+007"
         width="67"/>

        <check_box
         control_name="DisableCameraConstraints"
         height="16"
         initial_value="true"
         label="Disable camera constraints"
         layout="topleft"
         left="35"
         name="DisableCameraConstraints"
         tool_tip="Set this to move your camera beyond its normal bounds and below land level. If you are not good at moving the camera, more of your shots may be at bad angles."
         top_pad="10"
         width="315"/>
        <check_box
         control_name="FSDisableMouseWheelCameraZoom"
         height="16"
         label="Don't use the mouse wheel to control zoom level of the camera"
         layout="topleft"
         name="FSDisableMouseWheelCameraZoom"
         top_pad="2"
         width="315"/>
        <text
         follows="left|top"
         type="string"
         length="1"
         height="10"
         left="55"
         name="heading2"
         width="300"
         top_pad="8">
         Automatically pose avatar during (select one or more):
        </text>
        <check_box
         control_name="EditCameraMovement"
         height="20"
         follows="left|top"
         label="Build/Edit"
         layout="topleft"
         left_delta="10"
         name="edit_camera_movement"
         tool_tip="Use automatic camera positioning when entering and exiting edit mode"
         width="280"
         top_pad="5" />
        <check_box
         control_name="AppearanceCameraMovement"
         follows="left|top"
         height="16"
         label="Appearance"
         layout="topleft"
         name="appearance_camera_movement"
         tool_tip="Use automatic camera positioning while in edit mode"
         width="100" />
        <check_box
         enabled_control="AppearanceCameraMovement"
         control_name="EditAppearanceLighting"
         follows="left|top"
         height="16"
         label="Add Additional Lighting"
         layout="topleft"
         left_pad="10"
         name="EditAppearanceLighting"
         tool_tip="Enable or disable the additional lighting used while editing avatar appearance."
         width="150" />
        <check_box
         control_name="FSAppearanceShowHints"
         follows="left|top"
         height="16"
         label="Show Visual Hints"
         layout="topleft"
         left_pad="10"
         name="FSAppearanceShowHints"
         tool_tip="Enable or disable the visual hints showing the avatar parts being modified."
         width="150" />
        <text
         follows="left|top"
         type="string"
         length="1"
         height="10"
         left="38"
         name="presets_buttons_label"
         width="50"
         top_pad="10">
          Presets:
        </text>
        <button
         follows="top|left"
         height="23"
         label="Save"
         layout="topleft"
         left_pad="5"
         name="PrefCameraSaveButton"
         top_delta="-4"
         width="115">
         <button.commit_callback
          function="Pref.PrefSave"
          parameter="camera" />
        </button>
        <button
         follows="top|left"
         height="23"
         label="Load"
         layout="topleft"
         left_pad="10"
         name="PrefCameraLoadButton"
         top_delta="0"
         width="115">
         <button.commit_callback
          function="Pref.PrefLoad"
          parameter="camera"/>
        </button>
        <button
         follows="top|left"
         height="23"
         label="Delete"
         layout="topleft"
         left_pad="10"
         name="PrefCameraDeleteButton"
         top_delta="0"
         width="115">
         <button.commit_callback
          function="Pref.PrefDelete"
          parameter="camera"/>
        </button>
        <check_box
         top_pad="10"
         follows="left|top"
         height="16"
         initial_value="false"
         label="Clicking your avatar keeps camera position"
         layout="topleft"
         left="20"
         name="clickonavatarkeepscamera"
         width="270"
         control_name="ClickOnAvatarKeepsCamera"
         tool_tip="Normally, clicking on your avatar resets the camera position. This option removes this behavior."/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         initial_value="false"
         label="Reset camera position after teleport (within a region)"
         layout="topleft"
         left="20"
         name="FSResetCameraOnTP"
         width="270"
         control_name="FSResetCameraOnTP"
         tool_tip="Normally, the camera position gets reset when teleporting. This option prevents this behavior."/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         label="Reset camera position on avatar movement"
         layout="topleft"
         left="20"
         name="FSResetCameraOnMovement"
         width="270"
         control_name="FSResetCameraOnMovement"/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         initial_value="false"
         label="Disable minimum camera zoom distance"
         layout="topleft"
         left="20"
         name="disableminzoomdist"
         width="350"
         control_name="FSDisableMinZoomDist"
         tool_tip="Disable the constraint on the closest distance the camera is allowed to get to an object."/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         initial_value="false"
         label="Turn avatar towards camera direction on reset view"
         layout="topleft"
         left="20"
         name="resetviewturnsavatar"
         width="350"
         control_name="ResetViewTurnsAvatar"
         tool_tip="This option keeps the camera direction and turns the avatar when Reset View is selected (hit ESC key)."/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         initial_value="false"
         label="Allow the camera to move without constraints through prims"
         layout="topleft"
         left="20"
         name="ignorecameraconstraints"
         width="350"
         control_name="FSIgnoreSimulatorCameraConstraints"
         tool_tip="Ignores the 'push' the simulator applies to your camera to keep it out of objects."/>
        <text
         name="ignorecameraconstraints_requires_restart"
         follows="left|top"
         layout="topleft"
         left_pad="10"
         top_delta="0"
         height="20"
         width="130"
         text_color="White_25">
            (requires restart)
        </text>
        <check_box
         top_pad="-1"
         follows="left|top"
         height="16"
         label="Show the default camera controls minifloater always as opaque"
         layout="topleft"
         left="20"
         name="FSAlwaysOpaqueCameraControls"
         width="350"
         control_name="FSAlwaysOpaqueCameraControls"/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         label="Re-render temporary derendered objects after teleport"
         layout="topleft"
         left="20"
         name="FSTempDerenderUntilTeleport"
         width="350"
         tool_tip="If enabled, temporary derendered objects will stay derendered until teleport. If disabled, they stay derendered until the end of the session or get manually re-rendered via the asset blacklist window."
         control_name="FSTempDerenderUntilTeleport"/>
	</panel>

    <!--Mouselook-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="Mouselook"
     name="tab-mouselook">
        <icon
         follows="left|top"
         height="18"
         image_name="Move_Walk_Off"
         layout="topleft"
         name="avatar_icon"
         mouse_opaque="false"
         visible="true"
         width="18"
         top="10"
         left="20" />
        <check_box
         control_name="EnableMouselook"
         follows="top"
         height="20"
         initial_value="true"
         label="Enable Mouselook functionality"
         layout="topleft"
         left_pad="20"
         name="EnableMouselook"
         tool_tip="Allow first person perspective and mouse control of camera"
         width="256" />
        <check_box
         control_name="FirstPersonAvatarVisible"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         label="Show avatar in Mouselook"
         layout="topleft"
         left_delta="10"
         top_pad="8"
         name="first_person_avatar_visible"
         width="256" />
        <check_box
         control_name="FSShowInterfaceInMouselook"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         label="Show user interface in Mouselook"
         layout="topleft"
         name="first_person_ui_visible"
         top_pad="5"
         width="256" />
        <check_box
         control_name="FSShowConvoAndRadarInML"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         label="Don't close Conversations and Radar windows when changing into Mouselook"
         layout="topleft"
         name="FSShowConvoAndRadarInML"
         top_pad="5"
         width="256" />
        <check_box
         control_name="FSEnableRightclickMenuInMouselook"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         label="Enable context menus in Mouselook"
         layout="topleft"
         name="FSEnableRightclickMenuInMouselook"
         tool_tip="Opens pie/context menu in mouselook mode upon ALT + right mouse button"
         top_pad="5"
         width="256" />
        <check_box
         control_name="FSScrollWheelExitsMouselook"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         initial_value="true"
         label="Leave Mouselook using the scroll wheel"
         layout="topleft"
         name="FSScrollWheelExitsMouselook"
         tool_tip="If enabled, turning the scroll wheel on the mouse will exit Mouselook"
         top_pad="5"
         width="256" />
        <check_box
         control_name="FSShowMouselookInstructions"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         initial_value="true"
         label="Show Mouselook instructions"
         layout="topleft"
         name="mouselook_instructions_visible"
         tool_tip="Show Mouselook Instructions"
         top_pad="5"
         width="256" />
        <check_box
         control_name="ShowCrosshairs"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         initial_value="true"
         label="Show Mouselook crosshairs"
         layout="topleft"
         name="ShowCrosshairs"
         tool_tip="Display crosshairs when in mouselook mode"
         top_pad="5"
         width="256" />
        <check_box
         control_name="FSMouselookCombatFeatures"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="16"
         label="Enable combat features"
         layout="topleft"
         name="FSMouselookCombatFeatures"
         top_pad="5"
         width="256" />
        <check_box
         control_name="ExodusMouselookIFF"
         follows="left|top"
         height="16"
         label="Draw target markers"
         layout="topleft"
         left_delta="10"
         name="ExodusMouselookIFF"
         top_pad="5"
         width="256" />
        <slider
         control_name="ExodusMouselookIFFRange"
         follows="left|top"
         height="15"
         can_edit_text="true"
         decimal_digits="0"
         increment="5"
         layout="topleft"
         label="Range (m):"
         label_width="150"
         show_text="true"
         left_delta="5"
         max_val="500"
         name="ExodusMouselookIFFRange"
         top_pad="5"
         width="333" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="10"
         layout="topleft"
         left_delta="-10"
         name="Mouse Sensitivity"
         top_pad="10"
         width="160">
         Mouselook mouse sensitivity:
        </text>
        <slider
         control_name="MouseSensitivity"
         follows="left|top"
         enabled_control="EnableMouselook"
         height="15"
         initial_value="2"
         layout="topleft"
         show_text="false"
         left_pad="5"
         max_val="15"
         name="mouse_sensitivity"
         top_delta="1"
         width="145" />
        <check_box
         control_name="InvertMouse"
         enabled_control="EnableMouselook"
         height="16"
         label="Invert"
         layout="topleft"
         left_pad="2"
         name="invert_mouse"
         top_delta="0"
         width="128" />
	</panel>

    <!--Movement-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="Movement"
     name="tab-movement">
        <check_box
         control_name="ArrowKeysAlwaysMove"
         follows="left|top"
         height="20"
         label="Arrow keys always move me"
         layout="topleft"
         left_delta="10"
         name="arrow_keys_move_avatar_check"
         width="237"
         top_pad="8"/>
        <check_box
         control_name="LetterKeysAffectsMovementNotFocusChatBar"
         follows="left|top"
         height="20"
         label="Pressing letter keys affects movement (i.e. WASD) instead of starting local chat"
         layout="topleft"
         left_delta="0"
         name="LetterKeysAffectsMovementNotFocusChatBar"
         width="180"
         top_pad="0"/>
        <check_box
         control_name="FSUseAzertyKeyboardLayout"
         follows="left|top"
         height="20"
         label="Use AZERTY keyboard layout"
         layout="topleft"
         left_delta="0"
         name="FSUseAzertyKeyboardLayout"
         width="400"
         top_pad="0"/>
        <check_box
         control_name="AllowTapTapHoldRun"
         follows="left|top"
         height="20"
         label="Tap-tap-hold to run"
         layout="topleft"
         left_delta="0"
         name="tap_tap_hold_to_run"
         width="237"
         top_pad="0"/>
        <check_box
         control_name="AutomaticFly"
         follows="left|top"
         height="20"
         label="Fly / land on holding up (jump) / down (crouch)"
         layout="topleft"
         left_delta="0"
         name="automatic_fly"
         width="237"
         top_pad="0"/>
        <check_box
         control_name="FSCrouchToggle"
         follows="left|top"
         height="20"
         label="Enable crouch toggle mode"
         layout="topleft"
         left_delta="0"
         name="crouch_toggle_mode"
         width="237"
         top_pad="0"/>
        <check_box
         control_name="FSIgnoreFinishAnimation"
         follows="left|top"
         height="20"
         label="Disable waiting for pre-jump and landing animations"
         layout="topleft"
         left_delta="0"
         name="FSIgnoreFinishAnimation"
         width="237"
         top_pad="0"/>
        <check_box
         top_pad="0"
         follows="left|top"
         height="20"
         label="Allow avatars to walk backwards on your screen (SL default, local only)"
         name="FSDisableTurningAroundWhenWalkingBackwards"
         width="270"
         control_name="FSDisableTurningAroundWhenWalkingBackwards"
         tool_tip="When checked, this option allows avatars that are moving backwards to be seen on your screen as walking backwards. This setting only affects what your viewer shows, not what the avatar actually does. This is the default behavior in the Second Life viewer. The avatar's AO may override this behavior and force turning the avatar around instead."/>
        <check_box
         top_pad="0"
         follows="left|top"
         height="20"
         label="Turn avatar towards a selected object"
         name="FSTurnAvatarToSelectedObject"
         width="270"
         control_name="FSTurnAvatarToSelectedObject"
         tool_tip="Turns your avatar around to face the object you are currently selecting."/>
        <text
         follows="left|top"
         type="string"
         length="1"
         height="10"
         layout="topleft"
         left="10"
         name="movelock_lbl"
         width="450"
         top_pad="10">
         If built in LSL Bridge movelock is active...
        </text>
        <check_box
         control_name="RelockMoveLockAfterRegionChange"
         enabled_control="UseLSLBridge"
         follows="left|top"
         height="20"
         label="Always re-lock position after region change"
         layout="topleft"
         left_delta="5"
         name="RelockMoveLockAfterRegionChange"
         width="237"
         top_pad="5"/>
        <check_box
         control_name="RelockMoveLockAfterMovement"
         enabled_control="UseLSLBridge"
         follows="left|top"
         height="20"
         label="Lock and unlock position after stopping or starting movement"
         layout="topleft"
         left_delta="0"
         name="RelockMoveLockAfterMovement"
         width="237"
         top_pad="0"/>

        <text
         follows="left|top"
         type="string"
         length="1"
         height="10"
         layout="topleft"
         left="10"
         name="single_click_action_lbl"
         width="150"
         top_pad="20">
         Single click on land:
        </text>
        <combo_box
         height="23"
         layout="topleft"
         left_pad="10"
         top_delta="-6"
         name="single_click_action_combo"
         width="200">
            <combo_box.item
             label="No action"
             name="0"
             value="0"/>
            <combo_box.item
             label="Move to clicked point"
             name="1"
             value="1"/>
            <combo_box.commit_callback
             function="Pref.ClickActionChange"/>
        </combo_box>
        <text
         follows="left|top"
         type="string"
         length="1"
         height="10"
         layout="topleft"
         left="10"
         name="double_click_action_lbl"
         width="150"
         top_pad="10">
         Double click on land:
        </text>
        <combo_box
         height="23"
         layout="topleft"
         left_pad="10"
         top_delta="-6"
         name="double_click_action_combo"
         width="200">
            <combo_box.item
             label="No action"
             name="0"
             value="0"/>
            <combo_box.item
             label="Move to clicked point"
             name="1"
             value="1"/>
            <combo_box.item
             label="Teleport to clicked point"
             name="2"
             value="2"/>
            <combo_box.commit_callback
             function="Pref.ClickActionChange"/>
        </combo_box>
        <button
         follows="top|left"
         height="23"
         label="Joystick Configuration"
         left="100"
         name="joystick_setup_button"
         top_pad="15"
         width="155">
            <button.commit_callback
             function="Floater.Show"
             parameter="pref_joystick" />
        </button>
    </panel>

    <!-- Teleports -->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="Teleports"
     name="tab-teleports">

        <check_box
         control_name="PlayModeUISndTeleportOut"
         follows="left|top"
         height="20"
         label="Play sound effect when teleporting"
         layout="topleft"
         left_delta="10"
         name="PlayTeleportSound"
         width="237"
         top_pad="8"/>
        <check_box
         control_name="FSFlyAfterTeleport"
         follows="left|top"
         height="20"
         label="Always fly after teleporting"
         layout="topleft"
         name="FSFlyAfterTeleport"
         width="237"
         top_pad="0"/>
